@{
    Layout = null;
    var responses = ViewBag.Responses as List<string>;
}

<!DOCTYPE html>

<html>
<head>
    <title>Responses</title>
</head>
<body>
    <h1>Responses:</h1>

    @if (responses != null && responses.Count > 0)
    {
        foreach (var response in responses)
        {
            <p>@response</p>
        }
    }
    else
    {
        <p>No responses found.</p>
    }
</body>
</html>
